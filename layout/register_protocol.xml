<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/background">
    <include layout="@layout/login_top" />
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="@color/background">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="24dp"
                android:layout_marginTop="@dimen/d10"
                android:text="@string/regiter_protocol_title1"
                android:textColor="@color/title"
                android:textSize="@dimen/d18"/>
            <TextView
                android:id="@+id/content1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/d10"
                android:text="1"
                android:textColor="@color/content"
                android:padding="@dimen/d20"
                android:background="@color/white"
                android:textSize="@dimen/d18"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="24dp"
                android:layout_marginTop="@dimen/d10"
                android:text="@string/regiter_protocol_title2"
                android:textColor="@color/title"
                android:textSize="@dimen/d18"/>
            <TextView
                android:id="@+id/content2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/d10"
                android:text=""
                android:textColor="@color/content"
                android:padding="@dimen/d20"
                android:background="@color/white"
                android:textSize="@dimen/d18"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="24dp"
                android:layout_marginTop="@dimen/d10"
                android:text="@string/regiter_protocol_title3"
                android:textColor="@color/title"
                android:textSize="@dimen/d18"/>
            <TextView
                android:id="@+id/content3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/d10"
                android:text=""
                android:textColor="@color/content"
                android:padding="@dimen/d20"
                android:background="@color/white"
                android:textSize="@dimen/d18"/>
        </LinearLayout>
    </ScrollView>

</LinearLayout>
