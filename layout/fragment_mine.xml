<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/background">
    <com.zt.agw.view.widget.scrollview.OverScrollView
        android:id="@+id/scrollView1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/background">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="@color/background">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white">
                <com.zt.agw.view.widget.image.XCRoundImageView
                    android:id="@+id/img_head"
                    android:layout_width="@dimen/message_head_height"
                    android:layout_height="@dimen/message_head_height"
                    android:layout_alignParentLeft="true"
                    android:src="@drawable/head"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/d10"
                    android:layout_marginTop="@dimen/d20"
                    android:layout_marginBottom="@dimen/d20"
                    android:layout_marginLeft="@dimen/d10"/>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:orientation="vertical"
                    android:layout_marginTop="@dimen/d10"
                    android:layout_toRightOf="@+id/img_head"
                    android:layout_marginBottom="@dimen/d10">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/txt_name"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="秋秋"
                            android:textSize="@dimen/d18"
                            android:textColor="@color/black"/>
                        <ImageView
                            android:id="@+id/male_iv"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:src="@drawable/male"
                            android:layout_marginLeft="3dp"
                            android:layout_gravity="center_vertical"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/mark"
                            android:textSize="@dimen/d16"
                            android:layout_marginTop="@dimen/d10"
                            android:textColor="@color/content"/>
                        <TextView
                            android:id="@+id/txt_mark"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="30"
                            android:textSize="@dimen/d16"
                            android:layout_marginTop="@dimen/d10"
                            android:textColor="@color/orange"/>
                    </LinearLayout>
                </LinearLayout>
                <Button
                    android:id="@+id/register_bt"
                    android:layout_width="70dp"
                    android:layout_height="35dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/rounded_white_button"
                    android:text="@string/has_registered"
                    android:textColor="@color/middle_gray"
                    android:layout_marginRight="@dimen/d10"/>
            </RelativeLayout>
            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="@color/middle_gray"/>
            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:layout_marginTop="@dimen/d20"
                android:background="@color/middle_gray"/>
            <RelativeLayout
                android:id="@+id/my_send_rl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white">
                <ImageView
                    android:id="@+id/iv1"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@drawable/my_send"
                    android:layout_centerVertical="true"
                    android:layout_marginBottom="15dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginLeft="15dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/my_send"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/iv1"
                    android:layout_marginLeft="@dimen/d10"/>
                <ImageView
                    android:id="@+id/iv2"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@drawable/right_arrow"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/d10"/>
                <TextView
                    android:id="@+id/my_send_tv"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:background="@drawable/line_red"
                    android:layout_toLeftOf="@+id/iv2"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/d10"
                    android:text="5"
                    android:textColor="@color/white"
                    android:gravity="center"/>
            </RelativeLayout>
            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="@color/middle_gray"/>
            <RelativeLayout
                android:id="@+id/my_recieve_rl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white">
                <ImageView
                    android:id="@+id/r_iv1"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@drawable/my_recieve"
                    android:layout_centerVertical="true"
                    android:layout_marginBottom="15dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginLeft="15dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/my_recieve"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/r_iv1"
                    android:layout_marginLeft="@dimen/d10"/>
                <ImageView
                    android:id="@+id/r_iv2"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@drawable/right_arrow"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/d10"/>
                <TextView
                    android:id="@+id/my_recieve_tv"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:background="@drawable/line_red"
                    android:layout_toLeftOf="@+id/r_iv2"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/d10"
                    android:text="2"
                    android:textColor="@color/white"
                    android:gravity="center"/>
            </RelativeLayout>
            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="@color/middle_gray"/>
            <RelativeLayout
                android:id="@+id/my_comment_rl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white">
                <ImageView
                    android:id="@+id/c_iv1"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@drawable/my_edit"
                    android:layout_centerVertical="true"
                    android:layout_marginBottom="15dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginLeft="15dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/my_comment"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/c_iv1"
                    android:layout_marginLeft="@dimen/d10"/>
                <ImageView
                    android:id="@+id/c_iv2"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@drawable/right_arrow"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/d10"/>
                <TextView
                    android:id="@+id/my_comment_tv"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:background="@drawable/line_red"
                    android:layout_toLeftOf="@+id/c_iv2"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/d10"
                    android:text="10"
                    android:textColor="@color/white"
                    android:gravity="center"/>
            </RelativeLayout>
            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="@color/middle_gray"/>
            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:layout_marginTop="@dimen/d20"
                android:background="@color/middle_gray"/>
            <RelativeLayout
                android:id="@+id/my_mark_rl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white">
                <ImageView
                    android:id="@+id/m_iv1"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@drawable/mark_market"
                    android:layout_centerVertical="true"
                    android:layout_marginBottom="15dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginLeft="15dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/martk_market"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/m_iv1"
                    android:layout_marginLeft="@dimen/d10"/>
                <ImageView
                    android:id="@+id/m_iv2"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@drawable/right_arrow"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/d10"/>
            </RelativeLayout>
            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="@color/middle_gray"/>
            <RelativeLayout
                android:id="@+id/my_level_rl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white">
                <ImageView
                    android:id="@+id/l_iv1"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@drawable/my_level"
                    android:layout_centerVertical="true"
                    android:layout_marginBottom="15dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginLeft="15dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/my_level"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/l_iv1"
                    android:layout_marginLeft="@dimen/d10"/>
                <ImageView
                    android:id="@+id/l_iv2"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@drawable/right_arrow"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/d10"/>
            </RelativeLayout>
            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="@color/middle_gray"/>
            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:layout_marginTop="@dimen/d20"
                android:background="@color/middle_gray"/>
            <RelativeLayout
                android:id="@+id/setting_rl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white">
                <ImageView
                    android:id="@+id/s_iv1"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@drawable/setting"
                    android:layout_centerVertical="true"
                    android:layout_marginBottom="15dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginLeft="15dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/setting"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/s_iv1"
                    android:layout_marginLeft="@dimen/d10"/>
                <ImageView
                    android:id="@+id/s_iv2"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@drawable/right_arrow"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/d10"/>
            </RelativeLayout>
            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:layout_marginBottom="@dimen/d20"
                android:background="@color/middle_gray"/>
        </LinearLayout>
    </com.zt.agw.view.widget.scrollview.OverScrollView>
</LinearLayout>
